}

.sheet8:hover {
    background-image: url("project://database/Assets/Arts/UI/NoteSheet(6)-hover.png?fileID=2800000&guid=66503df5f9954dd459fcebdd1e3463cf&type=3#NoteSheet(6)-hover");
    -unity-background-image-tint-color: rgb(255, 255, 255);
    scale: 1.08 1.08;
    rotate: -1deg;
    transition-property: scale, rotate;
    transition-duration: 0.2s, 0.05s;
    transition-timing-function: ease-in-out-sine, ease-in-out-sine;
}

.detailpanel {
    opacity: 0;
}

.detailpanel:enabled {
    transition-property: opacity;
    transition-timing-function: ease-in-out-sine;
    transition-duration: 0.2s;
    opacity: 1;
}

